def Code(L,r=2):
	L = sorted(L)
	returnable = []
	result = ['0','1']
	for x in range(1,L[len(L)-1]+1):
		i = 0
		while i in range(0,L.count(x)):
			returnable.append(result[0])
			result.pop(0)
			i+=1
			aux = []
			for j in range(1,len(result)):
				aux.append(result(j)+'0')
				aux.append(result(j)+'1')
				result = aux
	return returnable
